    {
      title: "Contact",
      href: "/contact",
    },
  ],
  browse: [
    {
      title: "Blog",
      href: "/blog",
    },
    {
      title: "Twitter",
      href: "https://twitter.com/AshishK1331",
    },
  ],
};
